            hub.server.GetAllStores().done(function (datas) {

            $('#DataSource').empty();
                    ListStoreTemplate(datas);
                
            });
            hub.server.GetProductByIDStore(4, 'Food').done(function (datas) {
                //HomeSlider(datas);

            });


        }
        function HomeSlider(datas)
        {
            //$('#HomeSlider').empty();
            var msg = '';
            $.each(datas, function (i, obj) {
                msg +=
                '<div class="ma-carousel-item">' +
                    '<div class="ma-carousel-img" style="background-image:url(' + obj.imageUrl + ')"></div>' +
