        function search()
        {
            $('#DataSource').empty();
            var str = "";
            $(".CbTempatMakan option:selected").each(function () {
                str += $(this).val() + "||";
            });
            var allval = "";
            $(".CbTempatMakan option").each(function () {
                allval += $(this).val() + "||";
            });
            allarr = allval.split('||');
            arr = str.split('||');
            //var sortBy = $(".CbUrutkan").dropdown('get value');
            var arrFas = [];
            $("input:checkbox[name=CBFasilitas]:checked").each(function () {
                arrFas.push($(this).val());
            });

            $.each(arr, function (i, obj) {
                hub.server.GetStores("", obj, 0, 0, arrFas).done(function (datas) {
                    if (i != arr.length - 1)
                        ListStoreTemplate(datas);
                });
            });

