                    '</div>' +
                    '</div>';



            });
            $('#HomeSlider').append(msg);
        //$('#PanelKomentar').html(msg2);
        //SearchProductByIdStore();-


        }

        function ListStoreTemplate(datas) {

            var msg = "";
            $.each(datas, function (i, obj) {
                //facility
                var msgf = "";
                var pl = " ", pb = " ", pm = " ";
                $.each(obj.facilities, function (f, objf) {
                    var data = "";
                    switch (obj.facilities[f]) {
                        case "Parkir Luas":
                            {
                                pl = "active";
                            }
                            break;
                        case "Porsi Besar":
                            {
                                pb = "active";
                            }
                            break;
                        case "Prasmanan":
                            {
                                pm = "active";
                            }
                            break;
                    }

                });
                //end facility

                //rating
                var AvgMurahMeter = 0;
                var AvgKenikmatan = 0;
                $.each(obj.priceMeter, function (f, objm) {
                    AvgMurahMeter += objm.ratingValue;
                    AvgKenikmatan += obj.experience[f]['ratingValue'];
                });
                //berdasarkan angka rata2
                try {
                    AvgMurahMeter = AvgMurahMeter / obj.priceMeter.length;
                    AvgKenikmatan = AvgKenikmatan / obj.experience.length;

                    //berdasarkan persen
                    //AvgMurahMeter = AvgMurahMeter * 100 / 5;
